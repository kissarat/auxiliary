#!/usr/bin/env bash

# @author Taras Labiak <kissarat@gmail.com>

ROOT=`dirname $0`
cd ${ROOT}/..
export ROOT=$(pwd)
export PATH=${ROOT}/bin:${ROOT}/node_modules/.bin:${PATH}
if [[ -z ${NODE_ENV} ]]; then
  export NODE_ENV=development
fi

PS1="\e[33m\h\e[0m:\e[32m${PROJECT}\e[0m:\w$ " $1 $2 $3 $4 $5 $6 $7 $8 $9
